{
  "name": "server",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "build": "wrangler deploy --dry-run",
    "check-types": "tsc -b",
    "compile": "bun build --compile --minify --sourcemap --bytecode ./src/index.ts --outfile server",
    "dev": "wrangler dev --port=3000",
    "dev:local": "wrangler dev --port=3000",
    "dev:remote": "wrangler dev --remote --port=3000",
    "dev:persist": "wrangler dev --port=3000 --persist-to .wrangler/state",
    "start": "wrangler dev",
    "deploy": "wrangler deploy",
    "cf-typegen": "wrangler types --env-interface CloudflareBindings",
    "db:migrate:local": "wrangler d1 migrations apply DB --local",
    "db:migrate:remote": "wrangler d1 migrations apply auth-litewkateam --remote",
    "db:exec:local": "wrangler d1 execute DB --local --command",
    "db:exec:remote": "wrangler d1 execute auth-litewkateam --remote --command",
    "lint": "eslint ."
  },
  "dependencies": {
    "@my-better-t-app/api": "workspace:*",
    "@my-better-t-app/auth": "workspace:*",
    "@my-better-t-app/db": "workspace:*",
    "@orpc/openapi": "catalog:",
    "@orpc/server": "catalog:",
    "@orpc/zod": "catalog:",
    "@pdf-lib/fontkit": "^1.1.1",
    "better-auth": "catalog:",
    "dotenv": "catalog:",
    "drizzle-orm": "^0.44.7",
    "hono": "catalog:",
    "jspdf": "^3.0.4",
    "pdf-lib": "^1.17.1",
    "roboto-fontface": "^0.10.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "typescript": "catalog:",
    "tsdown": "catalog:",
    "@my-better-t-app/config": "workspace:*",
    "wrangler": "catalog:",
    "@types/node": "catalog:"
  }
}