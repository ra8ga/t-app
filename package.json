{
  "name": "my-better-t-app",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "hono": "^4.8.2",
      "@orpc/server": "^1.10.0",
      "@orpc/openapi": "^1.10.0",
      "@orpc/zod": "^1.10.0",
      "better-auth": "^1.3.28",
      "dotenv": "^17.2.2",
      "zod": "^4.1.11",
      "typescript": "^5.8.2",
      "tsdown": "^0.15.5",
      "wrangler": "^4.50.0",
      "@types/node": "^22.13.11",
      "@orpc/client": "^1.10.0",
      "drizzle-orm": "^0.44.2",
      "drizzle-kit": "^0.31.2",
      "@libsql/client": "^0.14.0",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "vite": "^6.2.2",
      "@vitejs/plugin-react": "^4.3.4",
      "@cloudflare/vite-plugin": "^1.13.8",
      "postcss": "^8.5.3",
      "tailwindcss": "^4.0.15",
      "@tailwindcss/vite": "^4.0.15",
      "@types/react": "~19.1.10",
      "@types/react-dom": "^19.0.4",
      "@tanstack/react-router": "^1.114.25",
      "@tanstack/react-router-devtools": "^1.114.27",
      "@tanstack/router-plugin": "^1.114.27",
      "@tanstack/react-query": "^5.85.5",
      "@tanstack/react-query-devtools": "^5.85.5",
      "@tanstack/react-form": "^1.12.3",
      "@orpc/tanstack-query": "^1.10.0",
      "@hookform/resolvers": "^5.1.1",
      "radix-ui": "^1.4.2",
      "class-variance-authority": "^0.7.1",
      "clsx": "^2.1.1",
      "lucide-react": "^0.473.0",
      "next-themes": "^0.4.6",
      "sonner": "^2.0.5",
      "tailwind-merge": "^3.3.1",
      "tw-animate-css": "^1.2.5",
      "@eslint/js": "^9.17.0",
      "eslint": "^9.17.0",
      "eslint-config-prettier": "^9.1.0",
      "eslint-plugin-react": "^7.37.2",
      "eslint-plugin-react-hooks": "^5.1.0",
      "eslint-plugin-react-refresh": "^0.4.16",
      "globals": "^15.14.0",
      "prettier": "^3.4.2",
      "prettier-plugin-tailwindcss": "^0.6.9",
      "typescript-eslint": "^8.18.1"
    }
  },
  "scripts": {
    "dev": "bun run dev:server:remote & bun run dev:web",
    "build": "turbo build",
    "dev:server": "turbo -F server dev",
    "dev:server:remote": "turbo -F server dev:remote",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "check-types": "turbo check-types",
    "dev:web": "turbo -F web dev",
    "catalog:check": "bun ./scripts/catalog-check.ts",
    "db:push": "turbo -F @my-better-t-app/db db:push:local",
    "db:push:local": "turbo -F @my-better-t-app/db db:push:local",
    "db:push:remote": "turbo -F @my-better-t-app/db db:push:remote",
    "db:studio": "turbo -F @my-better-t-app/db db:studio",
    "db:studio:local": "cd packages/db && bun run db:studio:local",
    "db:studio:remote": "cd packages/db && bun run db:studio:remote",
    "db:studio:local:file": "cd packages/db && bun run db:studio:local:file",
    "db:generate": "turbo -F @my-better-t-app/db db:generate:local",
    "db:generate:local": "turbo -F @my-better-t-app/db db:generate:local",
    "db:generate:remote": "turbo -F @my-better-t-app/db db:generate:remote",
    "db:migrate": "turbo -F @my-better-t-app/db db:migrate:local",
    "db:migrate:local": "turbo -F @my-better-t-app/db db:migrate:local",
    "db:migrate:remote": "turbo -F @my-better-t-app/db db:migrate:remote",
    "db:migrate:local:file": "cd packages/db && bun run db:migrate:local:file",
    "db:backup:remote": "bun ./scripts/db-utils.ts backup-remote",
    "db:backup:local": "bun ./scripts/db-utils.ts backup-local",
    "db:backup:full": "bun ./scripts/db-utils.ts backup-full",
    "db:clean-migrate:remote": "bun ./scripts/db-utils.ts clean-migrate-remote",
    "db:restore:local": "bun ./scripts/db-utils.ts restore-local",
    "db:restore:remote": "bun ./scripts/db-utils.ts restore-remote",
    "db:reset:local": "bun ./scripts/db-utils.ts reset-local",
    "db:restore:local:latest": "bun ./scripts/db-utils.ts restore-local-latest",
    "db:restore:remote:latest": "bun ./scripts/db-utils.ts restore-remote-latest",
    "db:cleanup:remote": "bun ./scripts/db-utils.ts cleanup-remote",
    "db:cleanup:local": "bun ./scripts/db-utils.ts cleanup-local",
    "db:clean-restore:remote:latest": "bun ./scripts/db-utils.ts clean-restore-remote-latest",
    "db:clean-restore:local:latest": "bun ./scripts/db-utils.ts clean-restore-local-latest",
    "db:clean-migrate:local": "bun ./scripts/db-utils.ts clean-migrate-local",
    "db:clean-migrate:local:file": "cd packages/db && bun run db:clean-migrate:local:file",
    "build:dev": "turbo -F web build:dev"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.15.2",
    "@my-better-t-app/config": "workspace:*",
    "@types/node": "^22.13.11",
    "bun-types": "^1.3.3",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "tsdown": "^0.15.5",
    "turbo": "^2.5.4"
  },
  "engines": {
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.2",
  "volta": {
    "node": "23.11.1",
    "npm": "10.9.2"
  }
}
