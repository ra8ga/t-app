{
  "name": "my-better-t-app",
  "private": true,
  "type": "module",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ],
    "catalog": {
      "hono": "^4.8.2",
      "@orpc/server": "^1.10.0",
      "@orpc/openapi": "^1.10.0",
      "@orpc/zod": "^1.10.0",
      "better-auth": "^1.3.28",
      "dotenv": "^17.2.2",
      "zod": "^4.1.11",
      "typescript": "^5.8.2",
      "tsdown": "^0.15.5",
      "wrangler": "^4.40.3",
      "@types/node": "^22.13.11",
      "@orpc/client": "^1.10.0",
      "drizzle-orm": "^0.44.2",
      "drizzle-kit": "^0.31.2",
      "@libsql/client": "^0.14.0",
      "react": "19.1.0",
      "react-dom": "19.1.0",
      "vite": "^6.2.2",
      "@vitejs/plugin-react": "^4.3.4",
      "@cloudflare/vite-plugin": "^1.13.8",
      "postcss": "^8.5.3",
      "tailwindcss": "^4.0.15",
      "@tailwindcss/vite": "^4.0.15",
      "@types/react": "~19.1.10",
      "@types/react-dom": "^19.0.4",
      "@tanstack/react-router": "^1.114.25",
      "@tanstack/react-router-devtools": "^1.114.27",
      "@tanstack/router-plugin": "^1.114.27",
      "@tanstack/react-query": "^5.85.5",
      "@tanstack/react-query-devtools": "^5.85.5",
      "@tanstack/react-form": "^1.12.3",
      "@orpc/tanstack-query": "^1.10.0",
      "@hookform/resolvers": "^5.1.1",
      "radix-ui": "^1.4.2",
      "class-variance-authority": "^0.7.1",
      "clsx": "^2.1.1",
      "lucide-react": "^0.473.0",
      "next-themes": "^0.4.6",
      "sonner": "^2.0.5",
      "tailwind-merge": "^3.3.1",
      "tw-animate-css": "^1.2.5"
    }
  },
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "check-types": "turbo check-types",
    "dev:native": "turbo -F native dev",
    "dev:web": "turbo -F web dev",
    "dev:server": "turbo -F server dev",
    "catalog:check": "bash -lc 'node ./scripts/catalog-check.cjs'",
    "db:push": "turbo -F @my-better-t-app/db db:push:local",
    "db:push:local": "turbo -F @my-better-t-app/db db:push:local",
    "db:push:remote": "turbo -F @my-better-t-app/db db:push:remote",
    "db:studio": "turbo -F @my-better-t-app/db db:studio",
    "db:studio:local": "bash -lc 'cd packages/db && bun run db:studio:local'",
    "db:studio:remote": "bash -lc 'cd packages/db && bun run db:studio:remote'",
    "db:studio:local:file": "bash -lc 'cd packages/db && bun run db:studio:local:file'",
    "db:generate": "turbo -F @my-better-t-app/db db:generate:local",
    "db:generate:local": "turbo -F @my-better-t-app/db db:generate:local",
    "db:generate:remote": "turbo -F @my-better-t-app/db db:generate:remote",
    "db:migrate": "turbo -F @my-better-t-app/db db:migrate:local",
    "db:migrate:local": "turbo -F @my-better-t-app/db db:migrate:local",
    "db:migrate:remote": "turbo -F @my-better-t-app/db db:migrate:remote",
    "db:migrate:local:file": "bash -lc 'cd packages/db && bun run db:migrate:local:file'",
    "db:backup:remote": "bash -lc 'bash ./scripts/db-utils.sh backup-remote'",
    "db:backup:local": "bash -lc 'bash ./scripts/db-utils.sh backup-local'",
    "db:backup:full": "bash -lc 'bash ./scripts/db-utils.sh backup-full'",
    "db:clean-migrate:remote": "bash -lc 'bash ./scripts/db-utils.sh clean-migrate-remote'",
    "db:restore:local": "bash -lc 'bash ./scripts/db-utils.sh restore-local \"${FILE:?set FILE}\"'",
    "db:restore:remote": "bash -lc 'bash ./scripts/db-utils.sh restore-remote \"${FILE:?set FILE}\"'",
    "db:reset:local": "bash -lc 'bash ./scripts/db-utils.sh reset-local'",
    "db:restore:local:latest": "bash -lc 'bash ./scripts/db-utils.sh restore-local-latest'",
    "db:restore:remote:latest": "bash -lc 'bash ./scripts/db-utils.sh restore-remote-latest'",
    "db:cleanup:remote": "bash -lc 'bash ./scripts/db-utils.sh cleanup-remote'",
    "db:cleanup:local": "bash -lc 'bash ./scripts/db-utils.sh cleanup-local'",
    "db:clean-restore:remote:latest": "bash -lc 'bash ./scripts/db-utils.sh clean-restore-remote-latest'",
    "db:clean-restore:local:latest": "bash -lc 'bash ./scripts/db-utils.sh clean-restore-local-latest'",
    "db:clean-migrate:local": "bash -lc 'bash ./scripts/db-utils.sh clean-migrate-local'",
    "db:clean-migrate:local:file": "bash -lc 'cd packages/db && bun run db:clean-migrate:local:file'"
  },
  "dependencies": {
    "dotenv": "^17.2.2",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "turbo": "^2.5.4",
    "tsdown": "^0.15.5",
    "@types/node": "^22.13.11"
  },
  "engines": {
    "node": ">=23",
    "bun": ">=1.3.0"
  },
  "packageManager": "bun@1.3.2"
}
