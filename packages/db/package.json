{
  "name": "@my-better-t-app/db",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./*": {
      "types": "./dist/*.d.ts",
      "default": "./src/*.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "db:push": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-push-local'",
    "db:push:local": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-push-local'",
    "db:push:remote": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-push-remote'",
    "db:studio": "drizzle-kit studio",
    "db:studio:local": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-studio-local'",
    "db:studio:remote": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-studio-remote'",
    "db:generate": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-generate-local'",
    "db:generate:local": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-generate-local'",
    "db:generate:remote": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-generate-remote'",
    "db:migrate": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-migrate-local'",
    "db:migrate:local": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-migrate-local'",
    "db:migrate:remote": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-migrate-remote'",
    "db:studio:local:file": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-studio-local-file'",
    "db:migrate:local:file": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-migrate-local-file'",
    "db:clean-migrate:local:file": "bash -lc 'bash ../../scripts/db-utils.sh drizzle-clean-migrate-local-file'"
  },
  "devDependencies": {
    "drizzle-kit": "catalog:",
    "tsdown": "catalog:",
    "@my-better-t-app/config": "workspace:*"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "engines": {
    "node": ">=23",
    "bun": ">=1.3.0"
  },
  "dependencies": {
    "drizzle-orm": "catalog:",
    "@libsql/client": "catalog:",
    "dotenv": "catalog:",
    "zod": "catalog:"
  }
}
